<div>
  <%= main_photo_thumb(@execution.school) -%>       
  <div class="school_list_meta">
    <h3><%= link_to "#{h(@execution.school.title)}",  project_execution_url(@execution.project,@execution), :target => "_blank" %></h3>
    <p>
      <span class="gary_span">
        <% if @school.geo.parent %>
        <%= h @school.geo.parent.name %>
        <% end %>
        <%= h @school.geo.name %>
      </span><br/>
      <span>项目状态: <%= state_tag(@execution.state)%></span>
    </p>
  </div>
</div>
