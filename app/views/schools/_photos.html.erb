<% photos.each do |p| %>
<dl class="obp">
  <dt><%= link_to image_tag(p.image.url, :alt => p.title), p.image.url(:max565x420), 
        :rel => "prettyPhoto[gallery]",:title => ''  %></dt>
  <dd>
    <%= link_to p.school.title, school_url(p.school) %>
    <span><%= link_to p.user.login, user_url(p.user) if p.user %></span>
  </dd>
</dl>
<% end -%>