<% @page_title = "公益产品管理" -%>

<% content_for :sidebar do -%>

  <%= render :partial => "/admin/public/operation_list" %>
<% end -%>

<div class="box">
  <div class="boxOuter">
    <div class="boxInner">
      <h3>创建新的公益产品/公益项目</h3>
      <div class="contents">
      <% form_for :project, :url => admin_requirement_types_url, :html => {:multipart => true} do |f| -%>
        <%= render :partial => "/requirement_types/form", :object => f %>
        <p><label></label><%= submit_tag "创建" %></p>
      <% end -%>
      </div>
    </div>
  </div>
</div>