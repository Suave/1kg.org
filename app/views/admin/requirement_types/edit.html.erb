<% @page_title = "公益产品管理" -%>

<% content_for :sidebar do -%>
  <h4>> <%= link_to "返回管理首页", admin_url %></h4>
  <%= render :partial => "/admin/shared/operation_list" %>
<% end -%>

<div class="box">
  <div class="boxOuter">
    <div class="boxInner">
      <h3>更新公益产品</h3>
      <div class="contents">
        <% form_for :project, :url => admin_requirement_type_url(@project), :html => {:method => :put, :multipart => true} do |f| %>
          <%= render :partial => "/requirement_types/form", :object => f %>
          <%= render :partial => "ext_form", :object => f %>
        <p><label></label><%= submit_tag "更新" %></p>
      <% end -%>
      </div>
    </div>
  </div>
</div>